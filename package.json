{
  "name": "proshop2",
  "version": "2.0.0",
  "type": "module",
  "description": "Ecommerce application built with the MERN stack",
  "main": "server.js",
  "scripts": {
    "start": "nodemon backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "dockerbuildwebapp": "concurrently \"npm run  build\" \"npm start\"",
    "build": "npm install && npm install --prefix frontend && npm run build --prefix frontend",
    "test": "mocha"
  },
  "author": "Brad Traversy",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "chalk": "^5.2.0",
    "colors": "^1.4.0",
    "config": "^3.3.9",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "elasticsearch": "^16.7.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.21.5",
    "@types/jest": "^29.5.1",
    "@types/supertest": "^2.0.12",
    "babel-jest": "^29.5.0",
    "chai": "^4.3.7",
    "concurrently": "^7.6.0",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0"
  }
}
